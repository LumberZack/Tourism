<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.touringvancouver">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FoodsActivity"
            android:label="@string/foodsCategory" />
        <activity android:name=".ParksActivity"
            android:label="@string/parksCategory"/>
        <activity android:name=".BeachesActivity"
            android:label="@string/beachesCategory"/>
        <activity android:name=".GamesActivity"
            android:label="@string/gamesCategory"/>
    </application>

</manifest>